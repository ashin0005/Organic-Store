<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Admin-customer</title>
  <link rel="shortcut icon" type="image/png" href="/admin/images/logos/favicon.png" />
  <link rel="stylesheet" href="/admin/css/styles.min.css" />
</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- including the sidebar,navbar -->
    <%- include ("admin-nav.ejs") %>
      <div class="container-fluid">
        <div class="card">
          <div class="card-body">
            <h1 class="fw-semibold mb-4">Customer</h1>
            <!-- table begin -->
            <div class="card w-100">
              <div class="card-body p-3">
                <h5 class="card-title fw-semibold mb-4">Customers Details</h5>
                <div class="table-responsive">
                  <table class="table text-nowrap mb-0 align-middle">
                    <thead class="text-dark fs-4">
                      <tr>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Id</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Name</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Gender</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">DOB</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Mobile No</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Address</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">City</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Landmark</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">State</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Zip code</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Email id</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Password</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Edit</h6>
                        </th>
                      </tr>
                    </thead>

                    <tbody>
                      <% cus.forEach(user=> { %>
                        <tr>
                          <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-0"> 1</h6>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.name %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.gender %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.dob %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.mob %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.address %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.city %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.landmark %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.state %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.pincode %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.emailid %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= user.password %>
                            </p>
                          </td>
                          <!-- <td class="border-bottom-0">
                          <button class="btn btn-light btn-edit"><i class="ti ti-edit"></i></button>
                          <button class="btn btn-primary btn-save" style="display: none;">Save</button>
                        </td> -->
                          <td class="border-bottom-0">
                            <button class="btn btn-light btn-edit"><i class="ti ti-edit"></i></button>
                            <button class="btn btn-primary btn-save" style="display: none;">Save</button>
                          </td>
                        </tr>
                        <% }); %>

                    </tbody>

                  </table>
                </div>
              </div>
            </div>
            <!-- table end -->
          </div>
        </div>
      </div>
  </div>
  </div>

  <script src="/admin/libs/jquery/dist/jquery.min.js"></script>
  <script src="/admin/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/admin/js/sidebarmenu.js"></script>
  <script src="/admin/js/app.min.js"></script>
  <script src="/admin/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="/admin/libs/simplebar/dist/simplebar.js"></script>
  <script src="/admin/js/dashboard.js"></script>
</body>

</html>