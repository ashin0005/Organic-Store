<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Admin-Order</title>
  <link rel="shortcut icon" type="image/png" href="/admin/images/logos/favicon.png" />
  <link rel="stylesheet" href="/admin/css/styles.min.css" />
  <style>
    * {
     user-select: none;
   }
 </style>
</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <%- include ("admin-nav.ejs") %>
      <!--  Header End -->
      <div class="container-fluid">
        <div class="card">
          <div class="card-body">
            <h1 class="fw-semibold mb-4">Order</h1>
            <!-- table begin -->
            <div class="card w-100">
              <div class="card-body p-3">
                <h5 class="card-title fw-semibold mb-4">Order list</h5>
                <div class="table-responsive">
                  <table class="table text-nowrap mb-0 align-middle">
                    <thead class="text-dark fs-4">
                      <tr>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Sl no</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Customer name</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Address</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Number</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Product Name</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Quantity</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Price</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Total</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Accept</h6>
                        </th>
                        <th class="border-bottom-0">
                          <h6 class="fw-semibold mb-0">Delete</h6>
                        </th>
                      </tr>
                    </thead>
                    <tbody>
                      <% ord.forEach(orders=> { %>
                        <tr>
                          <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">
                              <%= orders.slno %>
                            </h6>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= orders.name %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= orders.address %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= orders.number %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <p class="mb-0 fw-normal">
                              <%= orders.productname %>
                            </p>
                          </td>
                          <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-0 fs-4">
                              <%= orders.quantity %>
                            </h6>
                          </td>
                          <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-0 fs-4"> <%= orders.price %></h6>
                          </td>
                          <td class="border-bottom-0">
                            <h6 class="fw-semibold mb-0 fs-4"> <%= orders.total %></h6>
                          </td>
                          <td class="border-bottom-0">
                            <span><i class="ti ti-circle-check btn btn-outline-success"></i></span>
                          </td>
                          <td class="border-bottom-0">
                            <span><i class="ti ti-trash btn btn-outline-danger"></i></span>
                          </td>
                        </tr>
                        <% }); %>

                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <!-- table end -->

          </div>
        </div>
      </div>
  </div>
  </div>
  <script src="/admin/libs/jquery/dist/jquery.min.js"></script>
  <script src="/admin/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/admin/js/sidebarmenu.js"></script>
  <script src="/admin/js/app.min.js"></script>
  <script src="/admin/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="/admin/libs/simplebar/dist/simplebar.js"></script>
  <script src="/admin/js/dashboard.js"></script>
</body>

</html>